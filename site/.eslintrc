{
  "parser": "babel-eslint",
  "plugins": ["import", "react", "html"],
  "extends": ["standard"],
  "rules": {
    "class-methods-use-this": 0,
    "no-plusplus": 0,
    "import/extensions": 0,
    "import/prefer-default-export": 0,
    "no-param-reassign": 0,
    "no-undef": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "react/prop-types": 0,
    "@typescript-eslint/no-unused-vars": 0,
    "consistent-return": 0,
    "no-console": 0,
    "no-else-return": 0
  }
}
